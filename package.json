{
  "name": "edtech-demo",
  "version": "1.0.0",
  "description": "A simple REST API edtech demo",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development DEBUG=edtech-demo:* nodemon --exec babel-node -- src/server.ts --extensions '.ts,.tsx'",
    "release": "npm run clean && npm run build-app && npm run start",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run build-app",
    "build-app": "babel src -s -d dist --extensions '.ts,.tsx'",
    "start": "node dist/server.js",
    "test": "NODE_ENV=testing jest '--runInBand'",
    "test-single": "NODE_ENV=testing jest tests/user.test.ts",
    "coverage": "npm run test -- '--coverage'",
    "coverage:codacy": "npm run coverage && cp -R coverage .github/workflows/coverage/",
    "test-in-ci": "NODE_ENV=testing-in-ci jest '--runInBand'",
    "coverage-in-ci": "npm run test-in-ci -- '--coverage'",
    "coverage:codacy-in-ci": "npm run coverage-in-ci && cp -R coverage .github/workflows/coverage/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okezieobi/edtech-demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/okezieobi/edtech-demo/issues"
  },
  "homepage": "https://github.com/okezieobi/edtech-demo#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "cors": "latest",
    "debug": "latest",
    "dotenv": "latest",
    "express": "latest",
    "jsonwebtoken": "^8.5.1",
    "morgan": "latest",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.1",
    "sequelize-typescript": "^2.1.3",
    "strong-error-handler": "^4.0.0",
    "swagger-jsdoc": "latest",
    "swagger-ui-express": "latest"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/node": "latest",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/plugin-transform-typescript": "^7.17.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "latest",
    "@babel/register": "latest",
    "@tsconfig/recommended": "latest",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.36",
    "@types/cors": "latest",
    "@types/debug": "latest",
    "@types/express": "latest",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "latest",
    "@types/node": "latest",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "latest",
    "@types/swagger-ui-express": "latest",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "jest-extended": "^2.0.0",
    "nodemon": "latest",
    "supertest": "^6.2.3",
    "ts-node": "^10.5.0",
    "typescript": "latest"
  }
}
